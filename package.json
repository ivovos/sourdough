{
    "name": "webpack-vue",
    "version": "0.0.1",
    "description": "A Vue/Webpack/Cordova Bolierplate for prototyping",
    "main": "source/js/main.js",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack-dev-server --inline --hot ",
        "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
        "buildfast": "cross-env NODE_ENV=faste webpack --progress --hide-modules",
        "icon": "cordova-icon --icon=source/img/icon.png",
        "splash": "cordova-splash --splash=source/img/splash.png",
        "cordova": "node scripts/cordova/init.js && yarn build && yarn addiOS && yarn icon && yarn splash",
        "addiOS": "cordova platform add ios",
        "bumpCordova": "node scripts/cordova/versionBump.js $npm_package_version",
        "bump": "npm --no-git-tag-version version patch && yarn bumpCordova",
        "buildCordova": "yarn build && cordova prepare"
    },
    "keywords": [
        "cordova",
        "webpack",
        "vue",
        "boilerplate"
    ],
    "author": "Normally",
    "license": "ISC",
    "devDependencies": {
        "babel-core": "^6.23.1",
        "babel-loader": "^6.3.2",
        "babel-plugin-transform-runtime": "^6.22.0",
        "babel-preset-es2015": "^6.22.0",
        "babel-preset-stage-2": "^6.22.0",
        "clean-webpack-plugin": "^0.1.16",
        "copy-webpack-plugin": "^4.0.1",
        "cordova-icon": "^0.9.1",
        "cordova-splash": "^0.9.0",
        "cross-env": "^3.1.4",
        "css-loader": "^0.27.3",
        "favicons-webpack-plugin": "^0.0.7",
        "file-loader": "^0.10.1",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "fs-extra": "^3.0.1",
        "html-webpack-plugin": "^2.28.0",
        "image-webpack-loader": "^3.3.1",
        "node-sass": "^4.5.0",
        "prompt": "^1.0.0",
        "sass-loader": "^6.0.3",
        "style-loader": "^0.16.1",
        "template-file": "^1.0.4",
        "vue-loader": "^11.3.4",
        "vue-style-loader": "^2.0.5",
        "vue-template-compiler": "^2.2.6",
        "webpack": "^2.3.2",
        "webpack-dev-server": "^2.4.2"
    },
    "dependencies": {
        "cordova-ios": "^4.4.0",
        "cordova-plugin-console": "^1.0.7",
        "cordova-plugin-splashscreen": "^4.0.3",
        "cordova-plugin-statusbar": "^2.2.3",
        "cordova-plugin-whitelist": "^1.3.2",
        "fastclick": "^1.0.6",
        "inobounce": "^0.1.4",
        "vue": "^2.1.8",
        "vue-awesome-swiper": "^2.3.2"
    },
    "repository": "https://github.com/Normally/webpack-vue-cordova.git",
    "cordova": {
        "plugins": {
            "cordova-plugin-console": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-whitelist": {}
        },
        "platforms": [
            "ios"
        ]
    }
}
